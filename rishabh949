#include<stdio.h>

int main()
{
    int r,c;
    for(r=0;r<=4;r++)
    {
        for(c=0;c<4;c++)
        {
            if (r==0 && (c==1 || c==2))
            {printf("*");}
            else if (r!=0 && (c==0 || c==3))
            {printf("*");}
            else if (r==2 && (c==1 || c==2))
            {printf("*");}
            else
            {printf(" ");}
            
        }
        printf("\n");
    }
}
